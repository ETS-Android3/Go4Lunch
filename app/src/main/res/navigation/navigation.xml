<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.tonyocallimoutou.go4lunch.ui.mapview.MapViewFragment"
        android:label="@string/tabItem1"
        tools:layout="@layout/fragment_map_view" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.tonyocallimoutou.go4lunch.ui.listview.ListViewFragment"
        android:label="@string/tabItem2"
        tools:layout="@layout/fragment_list_view" />

    <fragment
        android:id="@+id/navigation_workmates"
        android:name="com.tonyocallimoutou.go4lunch.ui.workmate.WorkmateFragment"
